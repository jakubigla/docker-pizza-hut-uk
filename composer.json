{
    "name": "jakubigla/docker-pizza-hut",
    "description": "Automating Pizza Hut orders",
    "type": "project",
    "require": {
        "behat/mink": "^1.7",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink-goutte-driver": "^1.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8.1"
    },
    "autoload": {
        "psr-4": {
            "PizzaHut\\": "src/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
